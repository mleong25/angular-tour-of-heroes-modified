{
  "name": "angular-tour-of-heroes",
  "version": "0.0.0",
  "scripts": {
    "start": "npm-run-all --parallel start-mock-proxied start-mock-server",
    "start-mock-proxied": "ng serve --proxy-config mock.proxy.conf.json --disable-host-check --host 0.0.0.0",
    "start-mock-server": "mock-server",
    "start-server-proxied": "ng serve --proxy-config server.proxy.conf.json --disable-host-check --host 0.0.0.0",
    "build": "ng build --prod",
    "test": "ng test --prod",
    "coverage": "ng test --prod --code-coverage --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e --prod"
  },

  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "angular-in-memory-web-api": "^0.8.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@common/mock-server": "0.0.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
